class Solution {
public:
    void findPrimes(vector<int>& primes, int n) {
        
        for(int i = 2; i * i < n; i++) {
            if(primes[i]) {
                for(int j = i * i; j < n; j += i) {
                    primes[j] = 0;
                }
            }
        }
    }
    int countPrimes(int n) {
        if (n <= 2) return 0;
        vector<int> primes(n, 1);
        findPrimes(primes, n);
        primes[0] = primes[1] = 0;
        int cnt = 0;
        for(int i = 2; i < n; i++) {
            if(primes[i] == 1) {
                cnt++;
            }
        }
        return cnt;
    }
};
