    // bool hasSameDigits(string s) {
        
    //     string res = find_two(s);

    //     while(res.size() > 2) {
    //         res = find_two(res);
    //     }
    //     cout << res << endl;
    //     if(res.size() == 2 && res[0] == res[1]) return true;
    //     return false;
    // }

    bool hasSameDigits(string s) {
        int n = s.size();

        while(n > 2) {
            for(int i = 0; i < n - 1; i++) {
                s[i] = (s[i] - '0' + s[i + 1] - '0') % 10;
            }
            n--;
        }
        if(n == 2 && s[0] == s[1]) return true;
        return false;
    }
};
