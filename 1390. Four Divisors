class Solution {
public:
    // int findSum(int num) {
    //     int sum = 0, cnt = 0;

    //     for(int i = 1; i <= num; i++) {
    //         if(num % i == 0) {
    //             sum += i;
    //             cnt++;
    //         }
    //         if(cnt > 4) return 0;
    //     }

    //     if(cnt == 4) return sum;
    //     return 0;
    // }
    // int sumFourDivisors(vector<int>& nums) {
    //     int n = nums.size(), sum = 0;

    //     for(int i = 0; i < n; i++) {
    //         sum += findSum(nums[i]);
    //     }
    //     return sum;
    // }

    int findSum(int num) {
        int sum = 0, cnt = 0;

        for(int i = 1; i * i <= num; i++) {
            if(num % i == 0) {
                int divisor = num / i;

                if(i * i == num) {
                    sum += divisor;
                    cnt++;
                }else{
                    sum += i;
                    sum += divisor;
                    cnt += 2;
                }
                if(cnt > 4) return 0;
            }
        }
        if(cnt == 4) return sum;
        return 0;
    }

    int sumFourDivisors(vector<int>& nums) {
        int n = nums.size(), sum = 0;

        for(int i = 0; i < n; i++) {
            sum += findSum(nums[i]);
        }
        return sum;
    }
};
