class Solution {
public:

    // int f(int i, int j, vector<int>& nums1, vector<int>& nums2, vector<vector<int>>& dp) {
    //     if(i == nums1.size() || j == nums2.size()) return -1e9;
    //     if(dp[i][j] != -1) return dp[i][j];
    //     int val = nums1[i] * nums2[j];

    //     int pick_i_j = f(i + 1, j + 1, nums1, nums2, dp) + val;
    //     int pick_i = f(i + 1, j, nums1, nums2, dp);
    //     int pick_j = f(i, j + 1, nums1, nums2, dp);

    //     return dp[i][j] = max({val, pick_i_j, pick_i, pick_j});
    // }

    int tab(vector<int>& nums1, vector<int>& nums2) {
        int n = nums1.size(), m = nums2.size();
        vector<vector<int>> dp(n + 1, vector<int>(m + 1, -1e9));

        for(int i = 0; i < n; i++) {
            dp[i][0] = -1e9;
        }

        for(int j = 0; j < m; j++) {
            dp[0][j] = 1e9; 
        }

        for(int i = n - 1; i >= 0; i--) {
            for(int j = m - 1; j >= 0; j--) {
                int val = nums1[i] * nums2[j];

                int pick_i_j = dp[i + 1][j + 1] + val;
                int pick_i = dp[i + 1][j];
                int pick_j = dp[i][j + 1];

                dp[i][j] = max({val, pick_i_j, pick_i, pick_j});
            }
        }
        return dp[0][0];
    }
    
    int maxDotProduct(vector<int>& nums1, vector<int>& nums2) {
        // vector<vector<int>> dp(nums1.size(), vector<int>(nums2.size(), -1));
        // return f(0, 0, nums1, nums2, dp);
        return tab(nums1, nums2);
    }
};
